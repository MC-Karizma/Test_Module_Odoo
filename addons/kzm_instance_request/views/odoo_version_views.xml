    <menuitem name="Version" id="odoo_version_cat" parent="kzm_instance_request.menu_root"/>
    <!--<menuitem name="Menu 2" id="kzm_instance_request.menu_2" parent="kzm_instance_request.menu_root"/>-->

    <!-- actions -->

    <menuitem name="List" id="odoo_version_list" parent="odoo_version_cat"
              action="odoo_version_action_window" />
